{
  "name": "Clock",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"sass --watch src/styles:src/styles\"",
    "build": "vite build && npm run clean",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean": "del-cli src/styles/**/*.css.map",
